					case "FE-U-FLCTP": return "ad, soyad, şirket adı, telefon numarası ve şifre geçerli değil";
					case "FE-U-FLCEP": return "ad, soyad, şirket adı, email ve şifre geçerli değil";
					case "FE-U-FLTEP": return "ad, soyad, telefon numarası, email ve şifre geçerli değil";
					case "FE-U-FCTEP": return "ad, şirket adı, telefon numarası, email ve şifre geçerli değil";
					case "FE-U-LCTEP": return "soyad, şirket adı, telefon numarası, email ve şifre geçerli değil";

					// with six code
					case "FE-U-FLCTEP": return "ad, soyad, şirket adı, telefon numarası, email ve şifre geçerli değil";
					//#endregion

					//#region verification error codes
					case "VE-U-T": return "telefon numarası Yanlış";
					case "VE-U-P": return "şifre yanlış";
					//#endregion

					//#region conflict error codes
					case "CE-U-T": return "girilen telefon numarası daha önceden kullanılmış";
					case "CE-U-E": return "girilen email daha önceden kullanılmış";
					case "CE-U-TE": return "girilen telefon numarası ve email daha önceden kullanılmış";
					case "CE-U-M": return "girilen model daha önceden kaydedilmiş";
					//#endregion
					//////////////////////////////////////////
					//#endregion

					//#region machine error codes
					//////////////////////////////////////////
					//#region verification error codes
					case "VE-M-M": return "ana kategori bulunamadı";
					case "VE-M-S": return "alt kategori bulunamadı";
					case "VE-M-B": return "marka bulunamadı";
					case "VE-M-C": return "kategori bulunamadı";
					//#endregion

					//#region conflict error codes
					case "CE-M-M": return  "girilen model daha önceden kullanılmış";
					//#endregion
					//////////////////////////////////////////
					//#endregion

					//#region not found error codes
					case "NF-U": return "herhangi bir kullanıcı bulunamadı"
					case "NF-M": return "herhangi bir makine bulunamadı"
					//#endregion
				};
			}

			function getErrorMessage(responseText){
				var responseTextInJson = JSON.parse(responseText);

				return window.convertErrorCodeToErrorMessage(
					responseTextInJson["errorCode"]
				);
			}

			function writeErrorMessage(responseText, resultLabelId)
			{
				// convert error code to error message
				var responseTextInJson = JSON.parse(responseText);
				var errorMessage = convertErrorCodeToErrorMessage(
					responseTextInJson["errorCode"]);
				
				// show resultLabel
				$(resultLabelId).removeAttr("hidden");

				// write error
				updateResultLabel(errorMessage, "rgb(255, 75, 75)", resultLabelId);
			}

			function updateResultLabel(message, color, resultLabelId)
			{
				//#region reset resultLabel
				let resultLabel = $(resultLabelId);
				resultLabel.empty();
				//#endregion

				//#region change color
				resultLabel.attr("style",
					`color:${color}; 
					margin-top:30px; 
					margin-bottom: 30px;
					text-align: center`);
				//#endregion

				// show resultLabel
				$(resultLabelId).removeAttr("hidden");

				// update
				resultLabel.text(message);
			}

			function getDateTimeAsModified(dateTime) {
				//#region set year, hours and minutes
				let date = new Date(dateTime);

				let year = date.getFullYear();
				let hours = date.getHours() + 3;
				let minutes = date.getMinutes();
				//#endregion

				//#region set day
				let day = date.getDate();

				// add '0' to head
				let dayInString = day < 10 ?
					`0${day}`  // add 0
					: day.toString(); // don't add
				//#endregion

				//#region set month
				let month = date.getMonth() + 1;

				// add '0' to head
				let monthInString = month < 10 ?
					`0${month}`  // add 0
					: month.toString();  // don't add
				//#endregion

				return `${dayInString}.${monthInString}.${year} - ${hours}:${minutes}`;
			}

			function getHeaderInJson(headerName) {
				return JSON.parse(
					localStorage.getItem(headerName)
				);
			}
