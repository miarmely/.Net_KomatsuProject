		<script>
			function convertErrorCodeToErrorMessage(errorCode)
			{
				switch (errorCode) {
					//#region user error codes
					//////////////////////////////////////////
					//#region format error codes
					// with one code
					case "FE-U-F": return "ad geçerli değil";
					case "FE-U-L": return "soyad geçerli değil";
					case "FE-U-C": return "şirket adı geçerli değil";
					case "FE-U-T": return "telefon numarası geçerli değil";
					case "FE-U-E": return "email geçerli değil";
					case "FE-U-P": return "şifre geçerli değil";

					// with two code
					case "FE-U-FL": return "ad ve soyad geçerli değil";
					case "FE-U-FC": return "ad ve şirket adı geçerli değil";
					case "FE-U-FT": return "ad ve telefon numarası geçerli değil";
					case "FE-U-FE": return "ad ve email geçerli değil";
					case "FE-U-FP": return "ad ve şifre geçerli değil";
					case "FE-U-LC": return "soyad ve şirket adı geçerli değil";
					case "FE-U-LT": return "soyad ve telefon geçerli değil";
					case "FE-U-LE": return "soyad ve email geçerli değil";
					case "FE-U-LP": return "soyad ve şifre geçerli değil";
					case "FE-U-CT": return "şirket adı ve telefon geçerli değil";
					case "FE-U-CE": return "şirket adı ve email geçerli değil";
					case "FE-U-CP": return "şirket adı ve şifre geçerli değil";
					case "FE-U-TE": return "telefon numarası ve email geçerli değil";
					case "FE-U-TP": return "telefon numarası ve şifre geçerli değil";
					case "FE-U-EP": return "email ve şifre geçerli değil";

					// with three code
					case "FE-U-FLC": return "ad, soyad ve şirket adı geçerli değil";
					case "FE-U-FLT": return "ad, soyad ve telefon numarası geçerli değil";
					case "FE-U-FLE": return "ad, soyad ve email geçerli değil";
					case "FE-U-FLP": return "ad, soyad ve şifre geçerli değil";
					case "FE-U-FCT": return "ad, şirket adı ve telefon numarası geçerli değil";
					case "FE-U-FCE": return "ad, şirket adı ve email geçerli değil";
					case "FE-U-FCP": return "ad, şirket adı ve şifre geçerli değil";
					case "FE-U-FTE": return "ad, telefon numarası ve email geçerli değil";
					case "FE-U-FTP": return "ad, telefon numarası ve şifre geçerli değil";
					case "FE-U-FEP": return "ad, email ve şifre geçerli değil";
					case "FE-U-LCT": return "soyad, şirket adı ve telefon numarası geçerli değil";
					case "FE-U-LCE": return "soyad, şirket adı ve email geçerli değil";
					case "FE-U-LCP": return "soyad, şirket adı ve şifre geçerli değil";
					case "FE-U-LTE": return "soyad, telefon numarası ve email geçerli değil";
					case "FE-U-LTP": return "soyad, telefon numarası ve şifre geçerli değil";
					case "FE-U-LEP": return "soyad, email ve şifre geçerli değil";
					case "FE-U-CTE": return "şirket adı, telefon numarası ve email geçerli değil";
					case "FE-U-CTP": return "şirket adı, telefon numarası ve şifre geçerli değil";
					case "FE-U-CEP": return "şirket adı, email ve şifre geçerli değil";
					case "FE-U-TEP": return "telefon numarası, email ve şifre geçerli değil";

					// with four code
					case "FE-U-FLCT": return "ad, soyad, şirket adı ve telefon numarası geçerli değil";
					case "FE-U-FLCE": return "ad, soyad, şirket adı ve email geçerli değil";
					case "FE-U-FLCP": return "ad, soyad, şirket adı ve şifre geçerli değil";
					case "FE-U-FLTE": return "ad, soyad, telefon numarası ve email geçerli değil";
					case "FE-U-FLTP": return "ad, soyad, telefon numarası ve şifre geçerli değil";
					case "FE-U-FLEP": return "ad, soyad, email ve şifre geçerli değil";
					case "FE-U-FCTE": return "ad, şirket adı, telefon numarası ve email geçerli değil";
					case "FE-U-FCTP": return "ad, şirket adı, telefon numarası ve şifre geçerli değil";
					case "FE-U-FCEP": return "ad, şirket adı, email ve şifre geçerli değil";
					case "FE-U-FTEP": return "ad, telefon numarası, email ve şifre geçerli değil";
					case "FE-U-LCTE": return "soyad, şirket adı, telefon numarası ve email geçerli değil";
					case "FE-U-LCTP": return "soyad, şirket adı, telefon numarası ve şifre geçerli değil";
					case "FE-U-LCEP": return "soyad, şirket adı, email ve şifre geçerli değil";
					case "FE-U-LTEP": return "soyad, telefon numarası, email ve şifre geçerli değil";
					case "FE-U-CTEP": return "şirket adı, telefon numarası, email ve şifre geçerli değil";

